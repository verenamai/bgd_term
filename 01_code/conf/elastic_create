curl -XPUT 'http://localhost:9200/opportunities' -d '{
  "mappings": {
    "opportunities" : {
      "properties" : {
        "Id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "AccountId": {
          "type": "string",
          "index": "not_analyzed"
        },
        "Amount": {
          "type": "double"
        },
        "CloseDate": {
          "type": "date"
        },
        "LeadSource": {
          "type": "string",
          "index": "not_analyzed"
        },
        "Name": {
          "type": "string"
        },
        "OwnerId": {
          "type": "string",
          "index": "not_analyzed"
        },
        "StageName": {
          "type": "string",
          "index": "not_analyzed"
        },
        "TotalOpportunityQuantity": {
          "type": "double"
        },
        "Type": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  }
}'



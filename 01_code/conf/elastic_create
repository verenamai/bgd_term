Container connecten:
  docker exec -it opvizor-elk bash

Dann file erstellen
  vi predicted_tweets_all.json

Dann im VI copy paste
  :set paste
  i
  paste
  :w
kontrollieren ob alles passt dann
  :q

#leeren Index anlegen
curl -XPUT 'http://localhost:9200/predicted_tweets_all_attributes/'

#tweet einfügen mit mapping, falls vorhanden
 curl -vX POST 'http://localhost:9200/predictedtweets/_mapping/predictedtweets' –data-binary @predictedtweets.json

#tweet einfügen ohne mapping
 curl -vX POST 'http://localhost:9200/predictedtweets/tweet/1' -d @predictedtweets.json --header "Content-Type: application/json"
 curl -vX POST 'http://localhost:9200/predicted_tweets_all_attributes/tweet/1' -d @predicted_tweets_all.json --header "Content-Type: application/json"

# Inhalt in Elastic prüfen
  curl -XGET http://localhost:9200/predictedtweets/tweet/_search
  curl -XGET http://localhost:9200/predicted_tweets_all_attributes/tweet/_search
